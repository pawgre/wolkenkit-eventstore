{
  "name": "@pgrecki/wolkenkit-eventstore",
  "version": "0.0.9",
  "description": "wolkenkit-eventstore is an open-source eventstore for Node.js that is used by wolkenkit.",
  "scope": "pgrecki",
  "scripts": {
    "build": "mkdir -p ./dist && tsc --outDir ./dist --declaration",
    "pack": "cp package.json dist/ && cp .npmignore dist/ && cd dist/ && find . -name \"*.js.map\" -exec rm {} \\; && npm publish --access public",
    "git:pulltags": "git fetch --tags",
    "npm:release": "npm run git:pulltags; standard-version",
    "git:pushtags": "git push --follow-tags origin master",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:run": "npm i && npm i --no-save mysql2 && npm run lint",
    "test:unit": "mocha --check-leaks --exit \"src/**/*.test.ts\"",
    "test:integration": "mocha --check-leaks --unhandled-rejections=strict --exit \"test/integration/**/*.test.ts\"",
    "test:performance": "mocha --check-leaks --unhandled-rejections=strict --exit \"test/performance/**/*.test.ts\"",
    "test": "npm i && npm i --no-save mysql2 && npm run build && npm run lint && npm run test:unit && npm run test:integration && npm run test:performance",
    "audit": "npm audit --production --registry=https://registry.npmjs.org/ --audit-level=critical"
  },
  "contributors": [
    "Golo Roden <golo.roden@thenativeweb.io>",
    "Matthias Wagler <matthias.wagler@thenativeweb.io>",
    "Christian Nolte <christian.nolte@thenativeweb.io>",
    "Jan-Hendrik Grundhöfer <jan-hendrik.grundhoefer@thenativeweb.io>",
    "René Viering <rene.viering@gmail.com>"
  ],
  "dependencies": {
    "@types/async-retry": "~1.4.3",
    "@types/events": "~3.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.16",
    "async-retry": "~1.3.3",
    "commands-events": "~1.0.4",
    "dsn-parser": "1.0.3",
    "lodash": "^4.17.21",
    "tarn": "~3.0.2"
  },
  "devDependencies": {
    "uuid": "^8.3.2",
    "mocha": "^9.2.0",
    "assertthat": "~6.4.0",
    "measure-time": "~5.0.7",
    "streamtoarray": "~3.0.7",
    "standard-version": "^9.3.2",
    "eslint": "^8.2.0",
    "@types/uuid": "^8.3.4",
    "@types/mocha-each": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "typescript": "^4.5.5",
    "ts-node": "^10.5.0"
  },
  "peerDependencies": {
    "mongodb": "3.0.7",
    "mysql2": "^2.3.3",
    "pg": "7.4.1",
    "pg-query-stream": "1.1.1",
    "tedious": "2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pawgre/wolkenkit-eventstore.git"
  },
  "keywords": [
    "cqrs",
    "events",
    "eventsourcing",
    "eventstore",
    "wolkenkit"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/pawgre/wolkenkit-eventstore/issues"
  },
  "homepage": "https://github.com/pawgre/wolkenkit-eventstore#readme",
  "main": "index.js",
  "directories": {
    "dist": "dist"
  },
  "author": "Paweł Grecki"
}
